      fx:controller="cz.thewhiterabbit.electronicapp.view.controllers.QuickControlController"
      fx:id="quickControlMenu"
      stylesheets="@../stylesheets/style.css"
      styleClass="general_light">
    <Button fx:id="saveButton" styleClass="save-button" prefHeight="20" prefWidth="25"/>
    <Button fx:id="undoButton" styleClass="undo-button" prefHeight="20" prefWidth="25"/>
    <Button fx:id="redoButton" styleClass="redo-button" prefHeight="20" prefWidth="25"/>

</HBox>
